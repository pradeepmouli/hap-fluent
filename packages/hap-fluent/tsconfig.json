{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "ES2020",
		"module": "node18",
		"moduleResolution": "node16",
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"tsBuildInfoFile": "dist/.tsbuildinfo"
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist",
		"examples"
	]
}